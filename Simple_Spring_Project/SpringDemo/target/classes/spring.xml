<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Define your Spring beans here -->
    <bean id="alien" class="com.robin.SpringDemo.Alien" autowire="byType">
    <property name="age" value="10"></property>
    <!--<constructor-arg value="10"></constructor-arg> -->
    <!--<property name="com" ref="com"></property>-->
    </bean>
    
    <bean id="com" class="com.robin.SpringDemo.Laptop" primary="true"></bean>
    
    <bean id="desktop" class="com.robin.SpringDemo.Desktop"></bean>

</beans>